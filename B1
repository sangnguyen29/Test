#include<bits/stdc++.h>
#define MAX 100
using namespace std;

int n, a[MAX] = {0};
bool OK = false;

void init(){
	for(int i=1; i<=n; i++)
		cout << a[i] << " ";
	cout << endl;
}

void sinh(){
	int i = n;
	while(a[i]==1){
		a[i] = 0;
		i--;
	}
	if(i==0) OK = true;
	else a[i] = 1;
}
for
int main(){
	ios::base_sync_with_stdio(0);
	cin.tie(0);
	
	cin >>  n;
	while(!OK){
		init();
		sinh();
	}
}
